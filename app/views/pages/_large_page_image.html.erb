
<% if page %>
  <% content_for :head do %>
   <%= javascript_pack_tag "magnify" %>
  <% end %>

  <% content_for :large_page_image do %>
    <%= image_tag page.image.url(:original), id: "page-img", class: "img-responsive", data: { 'magnify-src' => page.image.url(:original) } %>
  <% end %>
<% end %>

